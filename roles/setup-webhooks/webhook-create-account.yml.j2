
- name: "Launches the '00 - Create Account' job template"
  hosts: localhost
  connection: local
  gather_facts: false

  tasks:
    - name: Execute every job template sequentially
      awx.awx.tower_job_launch:
      job_template: "00 - Create Account"
      extra_vars: "{{ lookup('file', '/tmp/create_account.json') }}"
      tower_host: "https://{{ awx_url }}"
      tower_oauthtoken: "{{ awx_session_token.ansible_facts.tower_token.token }}"
      validate_certs: yes
